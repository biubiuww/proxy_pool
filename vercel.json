{
    "version": 2,
    "engines": {
        "node": "18.x"
      },
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python",
        "config": {
          "python": "3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/proxyApi.py"
      }
    ]
  }
  